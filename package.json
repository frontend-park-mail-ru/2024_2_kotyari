{
  "name": "2024_2_kotyari",
  "version": "1.0.0",
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3"
  },
  "scripts": {
    "start": "node ./src/services/server/server.js"
  },
  "type": "module",
  "dependencies": {
    "express": "^4.21.0"
  },
  "scripts": {
    "prepare": "husky",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "npm run format && npm run lint -- --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
